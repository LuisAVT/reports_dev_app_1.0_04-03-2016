<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="escobedo.jnix.escobedo.emerg"
    android:background="#ffffff">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/emergency"
            android:background="#d63838"
            android:textColor="#ffffff"
            android:gravity="center_horizontal"
            android:textSize="25dp"
            android:textStyle="bold|normal"
            android:id="@+id/textView17" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton19"
            android:background="@drawable/cruz"
            android:layout_marginTop="26dp"
            android:layout_below="@+id/textView17"
            android:layout_alignLeft="@+id/imageButton21"
            android:layout_alignStart="@+id/imageButton21" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton20"
            android:background="@drawable/incendio"
            android:layout_marginTop="24dp"
            android:layout_below="@+id/imageButton19"
            android:layout_alignLeft="@+id/imageButton19"
            android:layout_alignStart="@+id/imageButton19" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton21"
            android:background="@drawable/emergencia"
            android:layout_marginLeft="26dp"
            android:layout_marginStart="26dp"
            android:layout_below="@+id/imageButton20"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="31dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton22"
            android:background="@drawable/denu"
            android:layout_marginBottom="33dp"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/imageButton21"
            android:layout_alignStart="@+id/imageButton21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cruz"
            android:textColor="#000000"
            android:textSize="25dp"
            android:id="@+id/textView21"
            android:layout_marginLeft="57dp"
            android:layout_marginStart="57dp"
            android:layout_marginBottom="29dp"
            android:layout_alignBottom="@+id/imageButton19"
            android:layout_toRightOf="@+id/imageButton19"
            android:layout_toEndOf="@+id/imageButton19" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/bombe"
            android:textColor="#000000"
            android:textSize="25dp"
            android:id="@+id/textView22"
            android:layout_marginBottom="23dp"
            android:layout_alignBottom="@+id/imageButton20"
            android:layout_alignLeft="@+id/textView21"
            android:layout_alignStart="@+id/textView21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/emer"
            android:textColor="#000000"
            android:textSize="25dp"
            android:id="@+id/textView23"
            android:layout_marginTop="28dp"
            android:layout_alignTop="@+id/imageButton21"
            android:layout_alignLeft="@+id/textView22"
            android:layout_alignStart="@+id/textView22" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/denus"
            android:id="@+id/textView24"
            android:textColor="#000000"
            android:textSize="25dp"
            android:layout_alignTop="@+id/imageButton22"
            android:layout_alignLeft="@+id/textView23"
            android:layout_alignStart="@+id/textView23"
            android:layout_marginTop="29dp" />


    </RelativeLayout>

    <TextView
        android:text="@string/hello2"
        android:layout_width="240dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="#FFD63838"/>

    <TextView
        android:text="@string/hello1"
        android:layout_width="240dp"
        android:layout_height="match_parent"
        android:layout_gravity="end"
        android:background="#FFD63838"/>

</android.support.v4.widget.DrawerLayout>
