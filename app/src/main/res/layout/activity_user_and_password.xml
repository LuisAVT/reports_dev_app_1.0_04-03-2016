    <RelativeLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <ImageView
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:id="@+id/imageView9"
             android:src="@drawable/escobedoletras"
             android:contentDescription="@string/el"
            android:layout_gravity="center_vertical|top" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextUsuario"
            android:backgroundTint="#ff0000"
            android:hint="@string/user"
            android:textColorHint="#ff9494"
            android:textColor="#fe3737"
            android:textSize="25dp"
            android:textStyle="normal"
            android:theme="@style/escobedoTheme"
            android:typeface="normal"
            android:layout_gravity="center"
            android:layout_centerVertical="true"
            android:layout_alignLeft="@+id/editText2Contra"
            android:layout_alignStart="@+id/editText2Contra" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editText2Contra"
            android:typeface="normal"
            android:theme="@style/escobedoTheme"
            android:textStyle="normal"
            android:textSize="25dp"
            android:textColor="#fe3737"
            android:textColorHint="#ff9494"
            android:backgroundTint="#ff0000"
            android:hint="Contraseña"
            android:layout_gravity="center"
            android:layout_below="@+id/editTextUsuario"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/inicio"
            android:id="@+id/button2Iniciar"
            android:background="#ffd9d9"
            android:textColor="#ff0000"
            android:textSize="25sp"
            android:theme="@style/escobedoTheme"
            android:typeface="monospace"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginTop="16dp"
            android:layout_below="@+id/editText2Contra"
            android:layout_centerHorizontal="true" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/regis"
            android:id="@+id/button3Registrarse"
            android:background="#ffd9d9"
            android:textColor="#ff0000"
            android:textSize="25sp"
            android:theme="@style/escobedoTheme"
            android:typeface="monospace"
            android:layout_marginTop="19dp"
            android:layout_below="@+id/button2Iniciar"
            android:layout_alignLeft="@+id/button2Iniciar"
            android:layout_alignStart="@+id/button2Iniciar"
            android:layout_alignRight="@+id/button2Iniciar"
            android:layout_alignEnd="@+id/button2Iniciar" />



    </RelativeLayout>

